	file = {PDF:/Users/bhargavrajyaguru/Zotero/storage/WPUKP7L7/Dadhich et al. - 2023 - Thermionic emission assisted charge conduction mechanisms across LaMnO3  La0.7Ca0.3MnO3 interface o.pdf:application/pdf},
}

