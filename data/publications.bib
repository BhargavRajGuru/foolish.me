	journal = {Solid State Communications},
	author = {Gal, Manan and Gadani, Keval and Dhruv, Davit and Joshi, Zalak and Zankat, Alpa and Rajyaguru, Bhargav and Joshi, A.D. and Asokan, K. and Solanki, P.S. and Shah, N.A.},
