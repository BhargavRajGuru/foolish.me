	file = {PDF:/Users/bhargavrajyaguru/Zotero/storage/6WDBH6UJ/Dadhich et al. - 2025 - Investigations on phenomenological percolation assisted charge conduction mechanisms across LaFeO3 .pdf:application/pdf},
